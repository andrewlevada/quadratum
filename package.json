{
  "name": "quadratum",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm run-script lint && cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "build:stats": "npm run-script lint && cross-env NODE_ENV=production:stats webpack --config webpack.prod.js --profile --json > stats.json",
    "start:dev": "cross-env NODE_ENV=development NODE_OPTIONS=\"--loader ts-node --experimental-modules\" npx webpack serve --hot -c webpack.dev.ts",
    "lint": "eslint --fix ./src",
    "deploy": "npm run build && firebase deploy"
  },
  "sideEffects": [
    "./src/pages/global.ts",
    "./src/utils/prototypes/*.ts"
  ],
  "dependencies": {
    "@firebase/app": "^0.7.20",
    "@firebase/auth": "^0.19.11",
    "@firebase/firestore": "^3.4.7",
    "@firebase/util": "^1.5.1",
    "@material/material-color-utilities": "^0.1.1",
    "@material/mwc-button": "^0.25.3",
    "@material/mwc-dialog": "^0.25.3",
    "@material/mwc-drawer": "^0.25.3",
    "@material/mwc-fab": "^0.25.3",
    "@material/mwc-icon": "^0.25.3",
    "@material/mwc-icon-button": "^0.25.3",
    "@material/mwc-menu": "^0.25.3",
    "@material/mwc-slider": "^0.25.3",
    "@material/mwc-textfield": "^0.25.3",
    "@sentry/browser": "^6.19.2",
    "@sentry/tracing": "^6.19.2",
    "lit": "^2.2.1",
    "md5": "^2.3.0"
  },
  "devDependencies": {
    "@sentry/webpack-plugin": "^1.18.8",
