    "html-webpack-plugin": "^5.5.0",
    "inspectpack": "^4.7.1",
    "lit-css-loader": "^1.2.2",
    "minify-html-literals-loader": "^1.1.1",
    "nodemon": "^2.0.15",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "simple-progress-webpack-plugin": "^2.0.0",
    "style-loader": "^3.3.1",
